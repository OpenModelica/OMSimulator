# Makefile generated by OpenModelica
CC=@CC@
AR=@AR@
CFLAGS=@CFLAGS@
LD=$(CC) -shared
LDFLAGS=@LDFLAGS@ @LIBS@
DLLEXT=@DLLEXT@
NEED_RUNTIME=@NEED_RUNTIME@
NEED_DGESV=@NEED_DGESV@
FMIPLATFORM=@FMIPLATFORM@
# Note: Simulation of the fmu with dymola does not work with -finline-small-functions (enabled by most optimization levels)
CPPFLAGS=@CPPFLAGS@
OMC_NUM_LINEAR_SYSTEMS=0
OMC_NUM_NONLINEAR_SYSTEMS=0
OMC_NUM_MIXED_SYSTEMS=0
OMC_NDELAY_EXPRESSIONS=0
OMC_NVAR_STRING=0

override CPPFLAGS += -Iinclude/ -Iinclude/fmi2 -I.    -DOMC_MODEL_PREFIX=cs_adder1 -DOMC_NUM_MIXED_SYSTEMS=0 -DOMC_NUM_LINEAR_SYSTEMS=0 -DOMC_NUM_NONLINEAR_SYSTEMS=0 -DOMC_NDELAY_EXPRESSIONS=0 -DOMC_NVAR_STRING=0

MAINFILE=cs_adder1_FMU.c
MAINOBJ=cs_adder1_FMU.o
CFILES=cs_adder1.c cs_adder1_functions.c cs_adder1_records.c \
cs_adder1_01exo.c cs_adder1_02nls.c cs_adder1_03lsy.c cs_adder1_04set.c cs_adder1_05evt.c cs_adder1_06inz.c cs_adder1_07dly.c \
cs_adder1_08bnd.c cs_adder1_09alg.c cs_adder1_10asr.c cs_adder1_11mix.c cs_adder1_12jac.c cs_adder1_13opt.c cs_adder1_14lnz.c \
cs_adder1_15syn.c cs_adder1_16dae.c cs_adder1_17inl.c cs_adder1_init_fmu.c
OFILES=$(CFILES:.c=.o)
GENERATEDFILES=$(MAINFILE) cs_adder1_FMU.makefile cs_adder1_literals.h cs_adder1_model.h cs_adder1_includes.h cs_adder1_functions.h  cs_adder1_11mix.h cs_adder1_12jac.h cs_adder1_13opt.h cs_adder1_init_fmu.c cs_adder1_info.c $(CFILES) cs_adder1_FMU.libs

# FIXME: before you push into master...
RUNTIMEDIR=include
OMC_MINIMAL_RUNTIME=1
OMC_FMI_RUNTIME=1
include $(RUNTIMEDIR)/Makefile.objs
ifneq ($(NEED_RUNTIME),)
RUNTIMEFILES=$(FMI_ME_OBJS:%=$(RUNTIMEDIR)/%.o)
endif

PHONY: cs_adder1_FMU
cs_adder1_FMU: $(MAINOBJ) cs_adder1_functions.h cs_adder1_literals.h $(OFILES) $(RUNTIMEFILES)
	mkdir -p ../binaries/$(FMIPLATFORM)
ifeq (@LIBTYPE_DYNAMIC@,1)
	$(LD) -o cs_adder1$(DLLEXT) $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)    $(LDFLAGS)
	cp cs_adder1$(DLLEXT) cs_adder1_FMU.libs config.log ../binaries/$(FMIPLATFORM)/
endif
ifeq (@LIBTYPE_STATIC@,1)
	rm -f cs_adder1.a
	$(AR) -rsu cs_adder1.a $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)
	cp cs_adder1.a cs_adder1_FMU.libs config.log ../binaries/$(FMIPLATFORM)/
endif
	$(MAKE) distclean
	cd .. && rm -f ../cs_adder1.fmu && zip -r ../cs_adder1.fmu *
distclean: clean
	rm -f Makefile config.status config.log
clean:
	rm -f cs_adder1.def cs_adder1.o cs_adder1.a cs_adder1$(DLLEXT) $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)